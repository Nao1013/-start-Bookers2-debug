<% if user_signed_in? %>
  <%= form_tag(saerch_path, method: :get) do %>
    <%= texr_field_tag "content" %>
    <%= select_tag "model", options_for_select({"User" => "user", "Book" => "book" }) %>
    <%= select_tag "method", options_for_select({ "完全一致" => "perfecr", "部分一致" => "partial", "前方一致" => "forward", "後方一致" => "backward" }) %>
    <%= submit_tag "検索" %>
  <% end %>
<% end %>